{
	"data": {
		"filename": "SingleSpeaker_LJSpeech_400s.csv",
		"visual_filename": "revise-SingleSpeaker-LibriSpeech32-test.csv",
		"visual_filename_columns":["wave"],
		"columns": [
			"LJSpeech_time"
		],
		"sequence_length": 640,
		"train_test_split": 0.85,
		"visualization_full_split": 0,
		"predict_seqlenght": 80,
		"normalise": true
	},
	"training": {
		"epochs": 50,
		"batch_size": 1024
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models/paper_revising",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 639,
				"input_dim": 1,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.4
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.4
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}